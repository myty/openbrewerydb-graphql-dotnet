{
  "name": "frontend",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "elliptic": "^6.5.3",
    "google-map-react": "^2.0.4",
    "history": "^5.0.0",
    "lodash": "^4.17.19",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-infinite-scroller": "^1.2.4",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "3.4.1",
    "tailwindcss": "^1.6.2",
    "heroicons-react": "^1.1.0"
  },
  "scripts": {
    "build:tailwind": "tailwind build src/tailwind.css -o src/tailwind.output.css",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/tailwind.output.css -c 'npm run build:tailwind'",
    "start": "npm-run-all build:tailwind --parallel schemas:watch watch:tailwind start:react",
    "start:react": "react-scripts start",
    "prebuild": "npm-run-all build:tailwind schemas",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "schemas": "graphql-codegen --config codegen.yml",
    "schemas:watch": "yarn schemas --watch",
    "prestart": "yarn schemas"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.7",
    "@graphql-codegen/import-types-preset": "^1.17.8",
    "@graphql-codegen/introspection": "1.17.7",
    "@graphql-codegen/typescript": "^1.17.7",
    "@graphql-codegen/typescript-operations": "^1.17.7",
    "@graphql-codegen/typescript-react-apollo": "^2.0.5",
    "@graphql-codegen/typescript-resolvers": "1.17.7",
    "@types/google-map-react": "^1.1.8",
    "@types/jest": "26.0.4",
    "@types/node": "14.0.23",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@types/react-infinite-scroller": "^1.2.1",
    "chokidar-cli": "^2.1.0",
    "graphql": "^15.3.0",
    "npm-run-all": "^4.1.5",
    "typescript": "3.9.7"
  }
}
