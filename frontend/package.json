{
    "name": "frontend",
    "version": "0.3.0",
    "private": true,
    "dependencies": {
        "@apollo/react-hooks": "^4.0.0",
        "@craco/craco": "^6.0.0",
        "@tailwindcss/postcss7-compat": "npm:@tailwindcss/postcss7-compat",
        "autoprefixer": "9",
        "elliptic": "^6.5.3",
        "geolib": "^3.3.1",
        "get-graphql-schema": "^2.1.2",
        "google-map-react": "^2.1.6",
        "heroicons-react": "^1.3.0",
        "history": "^5.0.0",
        "lodash": "^4.17.20",
        "postcss": "7",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-infinite-scroller": "^1.2.4",
        "react-router-dom": "^6.0.0-beta.0",
        "react-scripts": "4.0.0",
        "subscriptions-transport-ws": "^0.9.17",
        "tailwindcss": "npm:@tailwindcss/postcss7-compat",
        "uuidv4": "^6.2.6"
    },
    "scripts": {
        "build:tailwind": "tailwind build src/tailwind.css -o src/tailwind.output.css",
        "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/tailwind.output.css -c 'npm run build:tailwind'",
        "start": "npm-run-all build:tailwind --parallel schemas:watch watch:tailwind start:react",
        "start:react": "craco start",
        "prebuild": "npm-run-all build:tailwind schemas",
        "build": "craco build",
        "test": "craco test",
        "eject": "craco eject",
        "schemas": "graphql-codegen --config codegen.yml",
        "schemas:watch": "yarn schemas --watch",
        "prestart": "yarn schemas"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^1.19.0",
        "@graphql-codegen/import-types-preset": "^1.17.8",
        "@graphql-codegen/introspection": "1.18.0",
        "@graphql-codegen/typescript": "^1.17.7",
        "@graphql-codegen/typescript-operations": "^1.17.7",
        "@graphql-codegen/typescript-react-apollo": "^2.0.5",
        "@graphql-codegen/typescript-resolvers": "1.17.10",
        "@types/google-map-react": "^2.1.0",
        "@types/jest": "26.0.15",
        "@types/node": "14.14.6",
        "@types/react": "16.9.55",
        "@types/react-dom": "16.9.9",
        "@types/react-infinite-scroller": "^1.2.1",
        "@types/uuidv4": "5.0.0",
        "chokidar-cli": "^2.1.0",
        "graphql": "^15.4.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5",
        "typescript": "^4.1.3"
    },
    "volta": {
        "node": "14.15.4",
        "yarn": "1.22.10"
    }
}
